{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "build-debug",
		"command": "dotnet",
		"type": "shell",
		"args": [
		  "build",
		  "${workspaceFolder}/AirCombatMatchmakerBot.csproj",
		  "/property:GenerateFullPaths=true",
		  "/property:Configuration=DEBUG"
		],
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"presentation": {
		  "reveal": "silent"
		},
		"problemMatcher": "$msCompile"
	  },
	  {
		"label": "build-release",
		"command": "dotnet",
		"type": "shell",
		"args": [
		  "build",
		  "${workspaceFolder}/AirCombatMatchmakerBot.csproj",
		  "/property:GenerateFullPaths=true",
		  "/property:Configuration=RELEASE"
		],
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"presentation": {
		  "reveal": "silent"
		},
		"problemMatcher": "$msCompile"
	  }
	]
  }
  